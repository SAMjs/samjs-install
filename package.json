{
  "name": "samjs-install",
  "description": "A dynamic install server for samjs",
  "version": "0.2.1",
  "repository": "https://github.com/SAMjs/samjs-install",
  "author": {
    "name": "Paul Pflugradt",
    "email": "paul.pflugradt@gmail.com"
  },
  "main": "lib/main.js",
  "engines": {
    "node": "*"
  },
  "peerDependencies": {
    "samjs-client": "*"
  },
  "dependencies": {
    "apply-loader": "^0.1.0",
    "callback-loader": "^0.2.4",
    "coffee-script": "^1.11.1",
    "css-loader": "^0.25.0",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "koa": "^1.2.4",
    "koa-sendfile": "^2.0.0",
    "node-sass": "^3.10.1",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "vue": "^1.0.28",
    "vue-hot-reload-api": "^1.3.3",
    "vue-html-loader": "^1.2.3",
    "vue-icons": "^1.4.2",
    "vue-loader": "^8.5.4",
    "vue-materialize": "^1.0.0",
    "vue-style-loader": "^1.0.0",
    "vueify": "^8.7.0",
    "webpack": "^1.13.2",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.0"
  },
  "devDependencies": {
    "atom-ui-reporter": "0.0.1",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "coffee-loader": "^0.7.2",
    "mocha": "^3.1.2",
    "pug": "^2.0.0-beta6",
    "pug-cli": "^1.0.0-alpha6",
    "rimraf": "^2.5.4",
    "samjs": "^0.2.3",
    "samjs-client": "^0.2.2",
    "sass-loader": "^4.0.2",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "vue-compiler": "^1.0.0"
  },
  "keywords": [],
  "readmeFilename": "README.md",
  "scripts": {
    "prebuild": "rimraf lib/",
    "build": "run-npm build:*",
    "build:coffee": "coffee --no-header --compile --output lib/ src/*.coffee",
    "build:vue": "vue-compiler --out lib/ src/*.vue",
    "build:scss": "node-sass --output lib/ src/*.scss",
    "build:pug": "pug --out lib/ src/*.pug",
    "dev": "nodemon --watch src --watch dev dev/server.coffee",
    "test": "mocha",
    "preversion": "npm test",
    "version": "npm run build && git add .",
    "postversion": "git push && git push --tags && npm publish"
  }
}
